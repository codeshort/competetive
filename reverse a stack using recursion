
#include <iostream>
#include <stack>
using namespace std;
int n;
int p=0;
stack<int>  st;
void functioncall(int x)
{
    if(st.empty())
    {
        st.push(x);
        return;
    }
    int  head = st.top();
    st.pop();
    functioncall(x);
    st.push(head);
}
 void call()
 {
     if(st.empty())
     {
        return;
     }
     int fill = st.top();
     st.pop();
     call();
     functioncall(fill);
     
 }
int main()
{
     cin>>n;
    for(int i=0;i<n;i++)
    {
        int val;
        cin>>val;
        st.push(val);
    }
    call();
    while(!st.empty())
    {
        cout<<"top of stack:"<<st.top()<<endl;
        st.pop();
    }
    return 0;
}

